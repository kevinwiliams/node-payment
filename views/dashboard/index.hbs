<div class="container mt-5">
  <div class="row row-cols-1 row-cols-md-2 g-4">
    <!-- Categories Card -->
    <div class="col">
      <div class="card">
        <div class="card-header">
          Categories
        </div>
        <div class="card-body">
          <table class="table small">
            <thead>
              <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Description</th>
                <th>Active</th>
              </tr>
            </thead>
            <tbody>
        {{#each categories}}
              <tr>
                <td>{{dataValues.categoryId}}</td>
                <td>{{dataValues.name}}</td>
                <td>{{dataValues.description}}</td>
                <td>{{dataValues.active}}</td>
              </tr>
        {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>

